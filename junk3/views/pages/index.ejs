<!DOCTYPE html>
<html lang="en">
<head>
    <% include ../partials/head %>
   
</head>
<body class="container">

<header>
    <% include ../partials/header %>
</header>

<% include ../partials/jumbotron %>
 

<h2>Variable</h2>
<p><%= tagline %></p>

<h2>Loop</h2>
<table class= "table">
<% drinks.forEach(function(drink) { %>
<tr>
    <td>
        <%= drink.name %>
    </td>
    <td> <%= drink.drunkness %>
    </td>
</tr>
<% }); %>
</table>

<footer>
    <% include ../partials/footer %>
</footer>

</body>
</html>